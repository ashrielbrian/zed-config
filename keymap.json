[
    {
        "context": "Workspace",
        "bindings": {
            "cmd g g": [
                "task::Spawn",
                {
                    "lg": "Open lazygit"
                }
            ],
            "alt-`": "terminal_panel::ToggleFocus",
            "cmd-n t": "workspace::NewTerminal"
        }
    },
    {
        "context": "Editor",
        "bindings": {
            "cmd-shift-n": "editor::MovePageDown",
            "cmd-shift-m": "editor::MovePageUp"
        }
    }
]
